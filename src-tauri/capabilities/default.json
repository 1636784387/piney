{
    "identifier": "default",
    "description": "默认权限配置",
    "windows": [
        "main"
    ],
    "permissions": [
        "core:default",
        "process:default",
        "shell:allow-open",
        "dialog:default",
        {
            "identifier": "fs:default"
        },
        {
            "identifier": "fs:allow-read-text-file",
            "allow": [
                {
                    "path": "$APPDATA/**"
                },
                {
                    "path": "$DOWNLOAD/**"
                },
                {
                    "path": "$DOCUMENT/**"
                },
                {
                    "path": "$PICTURE/**"
                },
                {
                    "path": "$HOME/**"
                }
            ]
        },
        {
            "identifier": "fs:allow-write-text-file",
            "allow": [
                {
                    "path": "$APPDATA/**"
                },
                {
                    "path": "$DOWNLOAD/**"
                },
                {
                    "path": "$DOCUMENT/**"
                }
            ]
        },
        {
            "identifier": "fs:allow-read-file",
            "allow": [
                {
                    "path": "$APPDATA/**"
                },
                {
                    "path": "$DOWNLOAD/**"
                },
                {
                    "path": "$DOCUMENT/**"
                },
                {
                    "path": "$PICTURE/**"
                },
                {
                    "path": "$HOME/**"
                }
            ]
        },
        {
            "identifier": "fs:allow-write-file",
            "allow": [
                {
                    "path": "$APPDATA/**"
                },
                {
                    "path": "$DOWNLOAD/**"
                },
                {
                    "path": "$DOCUMENT/**"
                }
            ]
        },
        {
            "identifier": "fs:allow-mkdir",
            "allow": [
                {
                    "path": "$APPDATA/**"
                },
                {
                    "path": "$DOWNLOAD/**"
                }
            ]
        },
        {
            "identifier": "fs:allow-exists",
            "allow": [
                {
                    "path": "$APPDATA/**"
                },
                {
                    "path": "$DOWNLOAD/**"
                },
                {
                    "path": "$DOCUMENT/**"
                },
                {
                    "path": "$PICTURE/**"
                }
            ]
        }
    ]
}